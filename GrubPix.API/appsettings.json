{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "FrontendUrl": "__FRONTEND_URL__"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "__DB_CONNECTION_STRING__"
  },
  "AWS": {
    "AccessKey": "__AWS_ACCESS_KEY__",
    "SecretKey": "__AWS_SECRET_KEY__",
    "BucketName": "__AWS_BUCKET_NAME__",
    "Region": "__AWS_REGION__"
  },
  "JwtSettings": {
    "Secret": "__JWT_SECRET__",
    "Issuer": "__JWT_ISSUER__",
    "Audience": "__JWT_AUDIENCE__",
    "ExpiryMinutes": "__JWT_EXPIRY__"
  },
  "Clerk": {
    "SecretKey": "__CLERK_SECRET_KEY__",
    "BackendApiUrl": "__CLERK_BACKEND_URL__",
    "FrontendUrl": "__CLERK_FRONTEND_URL__",
    "Audience": "__CLERK_AUDIENCE__"
  },
  "Email": {
    "SmtpServer": "__EMAIL_SMTP_SERVER__",
    "Port": "__EMAIL_PORT__",
    "Username": "__EMAIL_USERNAME__",
    "Password": "__EMAIL_PASSWORD__",
    "From": "__EMAIL_FROM__"
  }
}
